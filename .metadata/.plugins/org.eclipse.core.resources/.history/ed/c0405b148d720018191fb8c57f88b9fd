package cm.allpha.Echo_Sos.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import cm.allpha.Echo_Sos.model.EchoSosPerson;
import cm.allpha.Echo_Sos.persistence.EchoSosPersonInterface;

@Service
public class EchoLbcPersonService {
	
	@Autowired
	EchoSosPersonInterface echoSosPersonInterface;
	
	public List<EchoSosPerson> getAllPerson() {
		return echoSosPersonInterface.findAll();
	}
}
